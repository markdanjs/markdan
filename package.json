{
  "name": "markdan",
  "version": "0.0.0",
  "packageManager": "pnpm@6.32.3",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/js2-43/markdan#readme",
  "bugs": {
    "url": "https://github.com/js2-43/markdan/issues"
  },
  "license": "MIT",
  "author": "chestnut <banlify@163.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:js2-43/markdan.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "esno ./scripts/build.ts dev",
    "build": "rimraf dist && esno ./scripts/build.ts",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@antfu/ni": "^0.13.2",
    "@babel/types": "^7.17.0",
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.7.10",
    "zx": "^6.0.7"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx,jsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
